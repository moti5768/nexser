        function nexser_start() {
            document.querySelector(&#039;.test_allwindow&#039;).style.display = &quot;none&quot;;
            window_none();
            if (localStorage.getItem(&#039;prompt_data2&#039;)) {
                setTimeout(function () {
                    console.log(largestZIndex)
                    prompt.style.display = &quot;none&quot;;
                    setTimeout(function () {
                        setColor()
                        startup_sound();
                        nexser.style.display = &quot;block&quot;;
                        setTimeout(function () {
                            taskbar_none();
                            desktop.style.display = &quot;block&quot;;
                            setTimeout(() =&gt; {
                                startup_window_open()
                                taskbtn_load()
                            }, 500);
                        }, 100);
                    }, 100);
                }, 100)
            } else {
                setTimeout(function () {
                    console.log(largestZIndex)
                    prompt.style.display = &quot;none&quot;;
                    setTimeout(function () {
                        setColor()
                        startup_sound();
                        nexser.style.display = &quot;block&quot;;
                        setTimeout(function () {
                            taskbar_none();
                            desktop.style.display = &quot;block&quot;;
